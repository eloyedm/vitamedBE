<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Listaalergias" table="listaAlergias">
    <indexes>
      <index name="U_Lista" columns="usuarioLA"/>
      <index name="A_Lista" columns="alergiaLA"/>
    </indexes>
    <id name="idlistaalergia" type="integer" column="idListaAlergia">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="alergiala" target-entity="Alergia">
      <join-columns>
        <join-column name="alergiaLA" referenced-column-name="idAlergia"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="usuariola" target-entity="Usuario">
      <join-columns>
        <join-column name="usuarioLA" referenced-column-name="idUsuario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
