<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Notificacion" table="notificacion">
    <indexes>
      <index name="C_Notificacion" columns="citaN"/>
    </indexes>
    <id name="idnotificacion" type="integer" column="idNotificacion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fechan" type="date" column="fechaN" nullable="true"/>
    <field name="horan" type="time" column="horaN" nullable="true"/>
    <field name="mensajen" type="string" column="mensajeN" length="25" nullable="true"/>
    <field name="estatusn" type="boolean" column="estatusN" nullable="true"/>
    <many-to-one field="citan" target-entity="Cita">
      <join-columns>
        <join-column name="citaN" referenced-column-name="idCita"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
