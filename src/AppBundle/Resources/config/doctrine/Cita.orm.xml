<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Cita" table="cita">
    <indexes>
      <index name="C_Cita" columns="consultorioC"/>
    </indexes>
    <id name="idcita" type="integer" column="idCita">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fechac" type="date" column="fechaC" nullable="true"/>
    <field name="horac" type="time" column="horaC" nullable="true"/>
    <field name="estatusc" type="boolean" column="estatusC" nullable="true"/>
    <field name="mensajec" type="string" column="mensajeC" length="25" nullable="true"/>
    <many-to-one field="consultorioc" target-entity="Consultorio">
      <join-columns>
        <join-column name="consultorioC" referenced-column-name="idConsultorio"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
