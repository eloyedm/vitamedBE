<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Listadoccon" table="listaDocCon">
    <indexes>
      <index name="D_ListaDC" columns="doctorLCD"/>
      <index name="C_ListaDC" columns="consultorioLCD"/>
    </indexes>
    <id name="idlistadoccon" type="integer" column="idListaDocCon">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="consultoriolcd" target-entity="Consultorio">
      <join-columns>
        <join-column name="consultorioLCD" referenced-column-name="idConsultorio"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="doctorlcd" target-entity="Doctor">
      <join-columns>
        <join-column name="doctorLCD" referenced-column-name="idDoctor"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
