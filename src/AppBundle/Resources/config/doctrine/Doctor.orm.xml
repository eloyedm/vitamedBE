<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Doctor" table="doctor">
    <id name="iddoctor" type="integer" column="idDoctor">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombred" type="string" column="nombreD" length="25" nullable="true"/>
    <field name="apellidomd" type="string" column="apellidoMD" length="25" nullable="true"/>
    <field name="apellidopd" type="string" column="apellidoPD" length="25" nullable="true"/>
    <field name="especialidad" type="string" column="especialidad" length="25" nullable="true"/>
  </entity>
</doctrine-mapping>
