<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuario" table="usuario">
    <id name="idusuario" type="integer" column="idUsuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sangreu" type="string" column="sangreU" length="5" nullable="true"/>
    <field name="fotou" type="blob" column="fotoU" length="65535" nullable="true"/>
    <field name="nombreu" type="string" column="nombreU" length="25" nullable="true"/>
    <field name="telefonou" type="string" column="telefonoU" length="25" nullable="true"/>
    <field name="contrau" type="binary" column="contraU" nullable="true"/>
    <field name="apellidomu" type="string" column="apellidoMU" length="25" nullable="true"/>
    <field name="apellidopu" type="string" column="apellidoPU" length="25" nullable="true"/>
    <field name="calleu" type="string" column="calleU" length="25" nullable="true"/>
    <field name="correou" type="string" column="correoU" length="50" nullable="true"/>
    <field name="coloniau" type="string" column="coloniaU" length="25" nullable="true"/>
    <field name="nocasau" type="string" column="noCasaU" length="10" nullable="true"/>
    <field name="paisu" type="string" column="paisU" length="25" nullable="true"/>
    <field name="estado" type="string" column="estado" length="25" nullable="true"/>
  </entity>
</doctrine-mapping>
